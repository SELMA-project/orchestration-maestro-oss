{
  "JobArchiver": {
    "Enabled": false,
    "OutputDirectory": ".",
    "Interval": "01:00",
    "MaxAge": "7.00:00",
    "OnlyArchiveCompletedJobs": false,
    "BatchSize": 1000,
    "RetryDelayMs": 30000
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "TimestampFormat": "HH:mm:ss dd-MM "
      }
    }
  }
}
